	"tokenColors": [
		// Python
		{
			"scope": [
				"source.python", // The whole source
				"punctuation.separator.period.python",
				"meta.attribute.python",
			],
			"settings": {
				"foreground": "#000c68",
			},
		},
		{
			"scope": [
				"entity.name.function.python",
				"meta.function-call.generic.python",
				"support.function.builtin.python",
				"constant.language.python",
				"constant.numeric.dec.python"
			],
			"settings": {
				"foreground": "#A900B9",
				"foreground": "#3535ed",
			},
		},
		{
			"scope": [
				"keyword.control.import.python",
				"storage.type.function.python",
				"keyword.control.flow.python",
				"meta.member.access.python",
				"keyword.operator.logical.python",
				"keyword.operator.assignment.python",
				"string.quoted.single.python",
			],
			"settings": {
				"foreground": "#921736",
			},
		},
		{
			"scope": [
				"meta.function-call.arguments.python",
			],
			"settings": {
				"foreground": "#285c79",
			},
		},
		{
			"scope": [],
			"settings": {
				"foreground": "#131c94",
			},
		},
		{
			"scope": [
				"entity.name.type.class.python",
				"variable.parameter.function.language.special.self.python",
				"variable.language.special.self.python",
			],
			"settings": {
				"foreground": "#A900B9",
			},
		},
		{
			"scope": [
				"variable.parameter.function.language.special.self.python",
				"variable.language.special.self.python",
			],
			"settings": {
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"comment.line.number-sign.python", // Comment Line
				"string.quoted.docstring.multi.python" // Docstring
			],
			"settings": {
				"foreground": "#7c7c7c",
				"fontStyle": "italic"
			},
		},
		// Markdown
		{
			"scope": [
				"heading.1.markdown", // Markdown Header 1 & 2
				"heading.2.markdown",
			],
			"settings": {
				"foreground": "#4caf50",
				"fontStyle": "bold underline",
			},
		},
		{
			"scope": [
				"heading.3.markdown", // Markdown Header 3 & 4
				"heading.4.markdown",
			],
			"settings": {
				"foreground": "#2196f3"
			},
		},
		{
			"scope": [
				"markup.list.unnumbered.markdown", // Markdown List Unnumbered
				"markup.list.numbered.markdown", // Markdown List Numbered
			],
			"settings": {
				"foreground": "#103a74"
			},
		},
		{
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": "#955cf4", // Markdown Block Code
			},
		},
		{
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#de3cad",
				"fontStyle": "bold"
			},
		},
		// SQL
		{
			"scope": [
				"support.function.aggregate.sql"
			],
			"settings": {
				"foreground": "#891f90", // Markdown Block Code
			},
		},
		{
			"scope": [
				"keyword.other.sql",
				"keyword.other.DML.sql",
				"keyword.plsql.mysql",
				"keyword.other.alias.sql"
			],
			"settings": {
				"foreground": "#b92032", // Markdown Block Code
			},
		},
		{
			"scope": [
				"string.quoted.double.sql",
			],
			"settings": {
				"foreground": "#1d5199", // SQL FIELDS
			},
		},
		{
			"scope": [
				"string.quoted.single.sql"
			],
			"settings": {
				"foreground": "#1d5199", // SQL STRING
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"punctuation.definition.comment.sql",
				"comment.line.double-dash.sql",
				"comment.block.c"
			],
			"settings": {
				"foreground": "#15a23d",
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"comment.line.double-dash.sql",
			],
			"settings": {
				"foreground": "#15a23d",
				"fontStyle": "italic underline"
			},
		},
	]
